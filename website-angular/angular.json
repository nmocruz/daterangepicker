{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "newProjectRoot": "projects",
    "projects": {
        "website-angular": {
            "root": "",
            "sourceRoot": "src",
            "projectType": "application",
            "prefix": "app",
            "schematics": {},
            "architect": {
                "build-webpack": {
                    "builder": "@angular-devkit/build-webpack:webpack",
                    "options": {
                        "webpackConfig": "webpack.config.js"
                    }
                },
                "serve-webpack": {
                    "builder": "@angular-devkit/build-webpack:webpack-dev-server",
                    "options": {
                        "webpackConfig": "webpack.config.js"
                    }
                },
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "options": {
                        "outputPath": "dist/website-angular",
                        "index": "src/index.html",
                        "main": "src/main.ts",
                        "polyfills": "src/polyfills.ts",
                        "tsConfig": "src/tsconfig.app.json",
                        "verbose": false,
                        "assets": [
                            "src/favicon.ico",
                            "src/assets",
                            {
                                "glob": "**/*",
                                "input": "../website/images/",
                                "output": "./assets/images/"
                            },
                            {
                                "glob": "**/*",
                                "input": "../website/styles/",
                                "output": "./assets/styles/"
                            },
                            {
                                "glob": "**/*.md",
                                "input": "node_modules/knockout-daterangepicker-fb/docs/",
                                "output": "./assets/docs/"
                            }
                        ],
                        "styles": [
                            "src/assets/styles/styles.scss",
                            "node_modules/prismjs/themes/prism-okaidia.css",
                            "node_modules/highlightjs/styles/github.css",
                            "../dist/daterangepicker.css",
                            "node_modules/mocha/mocha.css"
                        ],
                        "scripts": [
                            "node_modules/marked/lib/marked.js",
                            "node_modules/prismjs/prism.js",
                            "node_modules/prismjs/components/prism-css.min.js",
                            "node_modules/prismjs/components/prism-javascript.min.js",
                            "node_modules/prismjs/components/prism-markdown.min.js",
                            "node_modules/jquery/dist/jquery.min.js",
                            "node_modules/chai/chai.js",
                            "node_modules/mocha/mocha.js"
                        ]
                    },
                    "configurations": {
                        "production": {
                            "fileReplacements": [{
                                "replace": "src/environments/environment.ts",
                                "with": "src/environments/environment.prod.ts"
                            }],
                            "optimization": true,
                            "outputHashing": "all",
                            "extractCss": true,
                            "namedChunks": false,
                            "aot": true,
                            "extractLicenses": false,
                            "vendorChunk": false,
                            "buildOptimizer": true,
                            "sourceMap": {
                                "scripts": true
                            }
                        }
                    }
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "options": {
                        "browserTarget": "website-angular:build",
                        "progress": true
                    },
                    "configurations": {
                        "production": {
                            "browserTarget": "website-angular:build:production",
                            "extractLicenses": false
                        }
                    }
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "browserTarget": "website-angular:build"
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "src/test.ts",
                        "polyfills": "src/polyfills.ts",
                        "tsConfig": "src/tsconfig.spec.json",
                        "karmaConfig": "src/karma.conf.js",
                        "styles": ["src/assets/styles/styles.scss"],
                        "scripts": ["node_modules/knockout-daterangepicker-fb/dist/daterangepicker.umd.js"],
                        "assets": ["src/favicon.ico", "src/assets"]
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": ["src/tsconfig.app.json", "src/tsconfig.spec.json"],
                        "exclude": ["**/node_modules/**"]
                    }
                }
            }
        },
        "website-angular-e2e": {
            "root": "e2e/",
            "projectType": "application",
            "architect": {
                "e2e": {
                    "builder": "@angular-devkit/build-angular:protractor",
                    "options": {
                        "protractorConfig": "e2e/protractor.conf.js",
                        "devServerTarget": "website-angular:serve"
                    },
                    "configurations": {
                        "production": {
                            "devServerTarget": "website-angular:serve:production"
                        }
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": "e2e/tsconfig.e2e.json",
                        "exclude": ["**/node_modules/**"]
                    }
                }
            }
        }
    },
    "defaultProject": "website-angular",
    "schematics": {
        "@schematics/angular:component": {
            "styleext": "scss"
        }
    },
    "cli": {
        "packageManager": "yarn"
    }
}
